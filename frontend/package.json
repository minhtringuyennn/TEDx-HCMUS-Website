{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest --run",
    "lint": "eslint --cache --fix src/*/**.{tsx,ts}",
    "format": "prettier --write src/*.{tsx,ts}",
    "isready": "npm run lint && npm run format && npm run build && npm run test:run",
    "generate-icons": "npx @svgr/cli --config-file=.svgrrc.json ./src/assets/icons --out-dir=./src/icons",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "@tanstack/react-query": "4.29.14",
    "@tanstack/react-query-devtools": "4.29.14",
    "axios": "1.4.0",
    "date-fns": "2.30.0",
    "i18next": "23.2.0",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-i18next": "13.0.0",
    "react-redux": "8.1.0",
    "react-router-dom": "6.13.0",
    "styled-components": "6.0.0-rc.2-4007"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-angular": "17.6.5",
    "@storybook/addon-essentials": "7.0.22",
    "@storybook/addon-interactions": "7.0.22",
    "@storybook/addon-links": "7.0.22",
    "@storybook/blocks": "7.0.22",
    "@storybook/client-api": "7.0.22",
    "@storybook/react": "7.0.22",
    "@storybook/react-vite": "7.0.22",
    "@storybook/testing-library": "0.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.0.0",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/react-facebook-login": "4.1.5",
    "@types/testing-library__jest-dom": "5.14.6",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "0.32.2",
    "@vitest/ui": "0.32.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "9.1.2",
    "eslint": "8.43.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.12",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.2",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "storybook": "7.0.22",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.3",
    "unplugin-fonts": "^1.0.3",
    "vite": "4.3.9",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  },
  "lint-staged": {
    "*.{js,tsx,ts,css,md}": "prettier --write",
    "*.{js,tsx,ts}": "eslint --cache --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
